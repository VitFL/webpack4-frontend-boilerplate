{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "globals": {
    "chrome": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off"
  },
  "plugins": ["jquery", "babel", "prettier", "html"]
}